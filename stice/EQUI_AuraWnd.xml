			<OverlapBottom>0</OverlapBottom>
		</Border>
		<Titlebar>
			<Right>A_WindowTitleRight</Right>
			<Left>A_WindowTitleLeft</Left>
			<Middle>A_WindowTitleMiddle</Middle>
			<OverlapLeft>15</OverlapLeft>
			<OverlapTop>2</OverlapTop>
			<OverlapRight>15</OverlapRight>
			<OverlapBottom>0</OverlapBottom>
		</Titlebar>
